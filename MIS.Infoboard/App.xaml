<Application x:Class="MIS.Infoboard.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 StartupUri="Windows/MainWindow.xaml"
			 ShutdownMode="OnMainWindowClose"
			 DispatcherUnhandledException="App_DispatcherUnhandledException">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/Resources/ConvertersDictionary.xaml" />
				<ResourceDictionary Source="/Resources/StylesDictionary.xaml" />
				<ResourceDictionary Source="/Resources/TemplatesDictionary.xaml" />
				<ResourceDictionary Source="/Resources/TemplateSelectorsDictionary.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
